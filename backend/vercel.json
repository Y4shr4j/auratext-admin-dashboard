{
  "version": 2,
  "name": "auratext-admin-backend",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "API_KEY": "auratext_secret_key_2024_launch_secure",
    "DATABASE_PATH": "./analytics.db",
    "CORS_ORIGIN": "*"
  }
}
