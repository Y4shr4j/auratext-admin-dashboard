{
  "version": 2,
  "name": "auratext-admin-backend",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "API_KEY": "auratext_secret_key_2024_launch_secure",
    "DATABASE_PATH": "/tmp/analytics.db",
    "CORS_ORIGIN": "*",
    "PORT": "3001"
  }
}